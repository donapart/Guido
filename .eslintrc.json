{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_", "caughtErrorsIgnorePattern": "^_" }],
    "@typescript-eslint/no-explicit-any": "warn",
  "no-unused-vars": "off",
    "no-case-declarations": "off",
    "no-undef": "off",
  "no-constant-condition": "off",
    "prefer-const": "warn",
    "no-var": "error",
    "no-console": "off"
  },
  "ignorePatterns": [
    "out/**/*",
    "node_modules/**/*",
    "**/*.d.ts"
  ]
}
